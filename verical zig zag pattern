

n1=int(input())
b1=n1
a1=[[0 for i in range(n1)]for i in range(n1)]
for i in range(n1):
    for j in range(n1):
        a1[i][j]=b1
        b1+=n1
for i in range(n1):
    if i%2!=0:
        a1[i]=a1[i][::-1]
for i in range(n1):
    for j in range(n1):
        print(a1[j][i],end=" ")
    print()

        
